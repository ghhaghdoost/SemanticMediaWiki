{
	"description": "Test `_wpg/`_txt`",
	"setup": [
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has text",
			"contents": "[[Has type::Text]]"
		},
		{
			"namespace": "SMW_NS_PROPERTY",
			"page": "Has page",
			"contents": "[[Has type::Page]]"
		},
		{
			"page": "Example/Q0910/1/Foo bar/Bar",
			"contents": "[[Category:Q0910]] [[Has text::Foo bar/Bar]] [[Has page::Foo bar/Bar]]"
		},
		{
			"page": "Example/Q0910/1/Foobar/Bar/Foo",
			"contents": "[[Category:Q0910]] [[Has text::Foobar/Bar/Foo]] [[Has page::Foobar/Bar/Foo]]"
		},
		{
			"page": "Example/Q0910/1/Foo BAR/Foo",
			"contents": "[[Category:Q0910]] [[Has text::Foo BAR/Foo]] [[Has page::Foo BAR/Foo]]"
		},
		{
			"page": "Example/Q0910/1/Foo/Bar",
			"contents": "[[Category:Q0910]] [[Has text::Foo/Bar]] [[Has page::Foo/Bar]]"
		},
		{
			"page": "Example/Q0910/1/Foo",
			"contents": "[[Category:Q0910]] [[Has text::Foo]] [[Has page::Foo]]"
		},
		{
			"page": "Example/Q0910/2",
			"contents": "[[Category:Q0910]] {{#subobject: |Has page=Example/Q0910/2 |Has text=Example/Q0910/2 |Example/Q0910/2=A,B,C|+sep=, }}"
		}
	],
	"tests": [
		{
			"type": "query",
			"about": "#0 (~*bar*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[~*bar*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 4,
				"results": [
					"Example/Q0910/1/Foo bar/Bar#0##",
					"Example/Q0910/1/Foobar/Bar/Foo#0##",
					"Example/Q0910/1/Foo BAR/Foo#0##",
					"Example/Q0910/1/Foo/Bar#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#1 (~*Bar/*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[~*Bar/*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 3,
				"results": [
					"Example/Q0910/1/Foo bar/Bar#0##",
					"Example/Q0910/1/Foobar/Bar/Foo#0##",
					"Example/Q0910/1/Foo BAR/Foo#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#2 (~*Foo bar/*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[~*Foo bar/*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 2,
				"results": [
					"Example/Q0910/1/Foo bar/Bar#0##",
					"Example/Q0910/1/Foo BAR/Foo#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#3 (~F??Bar/*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[~*F??Bar/*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 1,
				"results": [
					"Example/Q0910/1/Foobar/Bar/Foo#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#4 (Has page:: ~*bar*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[Has page::~*bar*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 4,
				"results": [
					"Example/Q0910/1/Foo bar/Bar#0##",
					"Example/Q0910/1/Foobar/Bar/Foo#0##",
					"Example/Q0910/1/Foo BAR/Foo#0##",
					"Example/Q0910/1/Foo/Bar#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#5 (Has page::~*Bar/*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Category:Q0910]] [[Has page::~*Bar/*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 4,
				"results": [
					"Example/Q0910/1/Foo bar/Bar#0##",
					"Example/Q0910/1/Foobar/Bar/Foo#0##",
					"Example/Q0910/1/Foo BAR/Foo#0##",
					"Example/Q0910/1/Foo/Bar#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#6 (Has subobject::~*Q0910*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[Has subobject::~*Q0910*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 1,
				"results": [
					"Example/Q0910/2#0##"
				]
			}
		},
		{
			"type": "query",
			"about": "#7 (-Example/Q0910/2.-Has subobject::~*Q0910*)",
			"skip-on": {
				"elastic": [ "not", "Only works on ES out of the box." ]
			},
			"condition": "[[-Example/Q0910/2.-Has subobject::~*Q0910*]]",
			"printouts": [],
			"parameters": {
				"limit": "10"
			},
			"assert-queryresult": {
				"count": 3,
				"results": [
					"A#0##",
					"B#0##",
					"C#0##"
				]
			}
		}
	],
	"settings": {
		"smwgNamespacesWithSemanticLinks": {
			"NS_MAIN": true,
